<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.desmond.androidanimation" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.desmond.androidanimation.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.desmond.androidanimation.PropertyAnimation.PropertyAnimations"
            android:label="@string/title_activity_property_animations" >
        </activity>
        <activity
            android:name="com.desmond.androidanimation.DrawableAnimation.DrawableAnimation"
            android:label="@string/title_activity_drawable_animation" >
        </activity>
        <activity
            android:name="com.desmond.androidanimation.PropertyAnimation.Bounce"
            android:label="@string/title_activity_bounce" >
        </activity>
        <activity
            android:name="com.desmond.androidanimation.DrawableAnimation.KeyframeAnimation"
            android:label="@string/title_activity_keyframe_animation" >
        </activity>
        <activity
            android:name="com.desmond.androidanimation.DrawableAnimation.CrossFadingDrawables"
            android:label="@string/title_activity_cross_fading_drawables" >
        </activity>
        <activity
            android:name="com.desmond.androidanimation.WindowAnimation.WindowAnimations"
            android:label="@string/title_activity_window_animations" >
