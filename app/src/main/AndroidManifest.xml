            android:name="com.desmond.androidanimation.WindowAnimation.AnimatedSubActivity"
            android:label="@string/title_activity_animated_sub" >
        </activity>
        <activity
            android:name="com.desmond.androidanimation.ActivityAnimation.ActivityAnimations"
            android:label="@string/title_activity_activity_animations" >
        </activity>
        <activity
            android:name="com.desmond.androidanimation.ActivityAnimation.PictureDetailsActivity"
            android:label="@string/title_activity_picture_details"
            android:theme="@style/Transparent" >
        </activity>
        <activity
            android:name="com.desmond.androidanimation.MultiPropertyAnimations.MultiPropertyAnimations"
            android:label="@string/title_activity_multi_property_animations" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
